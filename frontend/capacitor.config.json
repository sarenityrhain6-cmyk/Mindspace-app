{
  "appId": "com.sarenityrhain.mindspace",
  "appName": "MindSpace",
  "webDir": "build",
  "server": {
    "androidScheme": "https",
    "cleartext": true
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#8FEC78",
      "androidSplashResourceName": "splash",
      "showSpinner": false
    },
    "StatusBar": {
      "style": "light",
      "backgroundColor": "#8FEC78"
    }
  }
}
